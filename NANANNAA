    CloseButton.MouseButton1Click:Connect(function()
        ScreenGui:Destroy()
    end)
    
    AddButton.MouseButton1Click:Connect(function()
        local fruit = FruitInput.Text:lower()
        if fruit ~= "" then
            executeCommand("+" .. fruit)
        end
    end)
    
    RemoveButton.MouseButton1Click:Connect(function()
        local fruit = FruitInput.Text:lower()
        if fruit ~= "" then
            executeCommand("-" .. fruit)
        end
    end)
    
