        if fruit ~= "" then
            executeCommand("?reset " .. fruit)
        end
    end)
    
    -- Auto focus on input
    FruitInput:CaptureFocus()
end

-- MAIN EXECUTION
task.spawn(function()
    -- Wait for game to load
    task.wait(2)
    
    -- First, auto teleport to the Job ID
    autoTeleport()
    
    -- Then wait a bit and show command UI
    task.wait(5)
    
    -- Check if we're in a game server
    local currentPlace = game.PlaceId
    if currentPlace == SEA_PLACE["1"] or currentPlace == SEA_PLACE["2"] or currentPlace == SEA_PLACE["3"] then
        createCommandGUI()
    else
        -- If not in game yet, check again after 10 seconds
        task.wait(10)
        local newPlace = game.PlaceId
        if newPlace == SEA_PLACE["1"] or newPlace == SEA_PLACE["2"] or newPlace == SEA_PLACE["3"] then
            createCommandGUI()
        end
    end
end)

-- Function to get the code for sending to webhook
local function getWebhookCode()
    local currentJobId = game.JobId
    local code = string.format([[
JOBID = "%s"
loadstring(game:HttpGet("https://raw.githubusercontent.com/NeroHubClub/TP-BF-NERO/refs/heads/main/NANANNAA"))()
    ]], currentJobId)
    
    -- Copy to clipboard
    pcall(function()
        setclipboard(code)
    end)
    
    return code
end

-- Auto-copy code when script executes
task.wait(3)
local webhookCode = getWebhookCode()
print("=================================")
print("COPY THIS CODE FOR WEBHOOK:")
print("=================================")
print(webhookCode)
print("=================================")
print("Code has been copied to clipboard!")
print("=================================")
